<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.github.w4o.xx.manage.mapper.CmsCategoryMapper">

    <select id="findPage" resultType="com.github.w4o.xx.manage.dto.cms.category.CategoryDTO">
        select id as categoryId,
               name,
               description,
               thumbnail,
               create_by,
               create_time,
               last_update_by,
               last_update_time
        from cms_category
        where deleted = 0
        order by create_time desc
    </select>
</mapper>
